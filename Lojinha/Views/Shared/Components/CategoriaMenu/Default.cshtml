<li class="dropdown">
    <a asp-controller="=Lanche" asp-action="List" class="dropdown-toggle" data-toggle="dropdown">
        Lanches
    </a>
    <ul class="dropdown-menu">
        @foreach(var categoria in Model)
        {
            <li>
                <a asp-controller="Lanche" asp-action="List"
                   asp-route-categoria="@categoria.CategoriaNome">@categoria.CategoriaNome</a>
            </li>
        }
        <li>
            <a asp-controller="Lanche" asp-action="List" asp-route-categoria="">Ver Lanches</a>
        </li>

    </ul>

</li>